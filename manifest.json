{
  
  "manifest_version": 2,
  "name": "EasyOnTheEyes",
  "version": "0.1",
  
  "description": "Simple reminder to take a break from the computer every 20 minutes for at least 20 seconds.",
  
  "browser_action": {
    "default_icon": {
      "48": "icons/timer-48.png",
      "96": "icons/timer-96.png"
    }
    "default_icon": "EasyOnTheEyes",
    "default_popup": "popup/index.html",
  }
  
  "contents_scripts": [
    {
      "matches": ["://*"],
      "js": ["easyOnTheEyes.js"]
    }
  ]
  
}